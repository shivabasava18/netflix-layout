{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/login.html",
      "dest": "/login.html"
    },
    {
      "src": "/register.html", 
      "dest": "/register.html"
    },
    {
      "src": "/netflix.html",
      "dest": "/netflix.html"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
